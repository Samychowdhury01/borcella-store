model User {
    id        String   @id @default(auto()) @map("_id") @db.ObjectId
    userId    String   @unique
    wishlist  String[] @default([]) @db.ObjectId
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

    model SignUp {
        id       String @id @default(auto()) @map("_id") @db.ObjectId
        email    String
        password String
        name     String

        @@unique([email])
    }
